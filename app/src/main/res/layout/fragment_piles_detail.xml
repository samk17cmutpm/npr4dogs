                        android:layout_alignParentTop="true"
                        android:layout_toRightOf="@+id/imageView40"
                        android:layout_toEndOf="@+id/imageView40"
                        android:layout_marginLeft="10dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:text="j.bates@yahoo.com"
                        android:id="@+id/textView120"
                        android:layout_below="@+id/textView119"
                        android:layout_toRightOf="@+id/imageView40"
                        android:layout_toEndOf="@+id/imageView40"
                        android:layout_marginLeft="10dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:text="Unit # 107"
                        android:id="@+id/textView121"
                        android:layout_below="@+id/textView120"
                        android:layout_toRightOf="@+id/imageView40"
                        android:layout_toEndOf="@+id/imageView40"
                        android:layout_marginLeft="10dp" />

                </RelativeLayout>

                </com.andexert.library.RippleView>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="Australian Shepherd"
                android:id="@+id/textView115"
                android:layout_alignTop="@+id/textView114"
                android:layout_toRightOf="@+id/textView114"
                android:layout_toEndOf="@+id/textView114" />


        </RelativeLayout>


    </RelativeLayout>

</FrameLayout>
