                android:id="@+id/textView113"
                android:layout_below="@+id/textView111"
                android:layout_toRightOf="@+id/textView112"
                android:layout_toEndOf="@+id/textView112"
                android:layout_marginTop="5dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="Coloring: "
                android:id="@+id/textView114"
                android:layout_alignLeft="@+id/textView112"
                android:layout_alignStart="@+id/textView112"
                android:textStyle="bold"
                android:layout_marginTop="5dp"
                android:layout_below="@+id/textView113" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="Shots: "
                android:id="@+id/textView116"
                android:layout_below="@+id/textView114"
                android:layout_alignLeft="@+id/textView114"
                android:layout_alignStart="@+id/textView114"
                android:textStyle="bold"
                android:layout_marginTop="5dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="Up to date"
                android:id="@+id/textView117"
                android:layout_below="@+id/textView114"
                android:layout_toRightOf="@+id/textView116"
                android:layout_toEndOf="@+id/textView116"
                android:layout_marginTop="5dp" />

            <ImageView
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:id="@+id/imageView38"
                android:src="@drawable/splash_screen_dog"
                android:layout_alignBottom="@+id/textView117"
                android:layout_alignRight="@+id/textView104"
                android:layout_alignEnd="@+id/textView104" />

            <ImageView
                android:layout_width="130dp"
