                android:layout_toRightOf="@+id/textView108"
                android:layout_toEndOf="@+id/textView108" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:background="@color/background"
                android:id="@+id/relativeLayout8"
                android:layout_below="@+id/textView116"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginTop="10dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="Owner"
                    android:id="@+id/textView118"
                    android:layout_centerVertical="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_marginLeft="10dp" />
            </RelativeLayout>

            <com.andexert.library.RippleView
                android:id="@+id/rvp_residents"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                rv_centered="true"
                android:layout_weight="0.37"
                android:padding="10dp"
                android:background="@drawable/cancle_backgrounnd"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/relativeLayout8">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <ImageView
                        android:layout_width="70dp"
                        android:layout_height="80dp"
                        android:id="@+id/imageView40"
